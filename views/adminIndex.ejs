    <button type="button" class="btn btn-primary btn-dark btn-lg">
      Open Course Allocation Session
    </button>

            <hr>
            <div style="margin: auto; align-items: baseline;">
                  <h4>Set Parameters to calculate Workload</h4>
                  <p>workload is calculated as a*L + b*T + c*P.</p>
                  <p style="display: inline-block;">a</p>
                  <textarea name="a" id="" cols="4" rows="1"></textarea>
                  <br>
                  <p style="display: inline-block;">b</p>
                  <textarea name="b" id="" cols="4" rows="1"></textarea>
                  <br>
                  <p style="display: inline-block;">c</p>
                  <textarea name="c" id="" cols="4" rows="1"></textarea>
            </div>

            <!-- Upload excel file -->
            <h2>Upload Excel file</h2><br>
            <form action="/admin/index" enctype="multipart/form-data" method="post">
                  <input type="file" name="uploadfile"
                        accept='application/vnd.openxmlformats-officedocument.spreadsheetml.sheet, application/vnd.ms-excel'>
                  <input type="submit" value="Upload Excel">
            </form>
            <!-- End of Upload excel file -->
            <hr>
      </div>

      <button type="button" class="btn btn-dark">Manage Courses</button>
